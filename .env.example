# Essentia API Configuration
# Copy this file to .env and update with your values

# API Server Configuration
API_HOST=0.0.0.0
API_PORT=8000

# CORS Configuration
# For production, set to specific domain(s): https://yourdomain.com
# For development, you can use * to allow all origins
CORS_ORIGINS=*

# API Authentication
# Comma-separated list of valid API keys
# Contact administrator for API keys
API_KEYS=your_api_key_here

# Essentia Models Path (for Docker deployment)
ESSENTIA_MODELS_PATH=/app/models
